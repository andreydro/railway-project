<tr>
  <td><%= ticket.train.number %></td>
  <td><%= ticket.start_station.title %></td>
  <td><%= ticket.end_station.title %></td>
  <td>
    <%= ticket.passenger_name %>
    <%= ticket.passenger_passport %>
  </td>
  <td>
    <%= link_to "Show", [:admin, ticket] %>
    <%= link_to "Edit", edit_admin_ticket_path(ticket) %>
    <%=	link_to "Destroy", [:admin, ticket], method: :delete, data: { confirm: "Are you sure?" } %>
  </td>
</tr>